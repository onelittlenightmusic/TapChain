<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.tapchain"
      android:versionCode="1"
      xmlns:android="http://schemas.android.com/apk/res/android" android:versionName="1.2.1">
    <uses-permission android:name="android.permission.VIBRATE"></uses-permission>
    <uses-permission android:name="android.permission.RECORD_AUDIO"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera"/>
    <uses-feature android:name="android.hardware.camera.autofocus"/>
    <application android:icon="@drawable/newframe" android:label="@string/app_name">
        <activity android:name=".TapChainView" android:label="@string/app_name" android:configChanges="keyboardHidden|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    <activity android:name="org.tapchain.Activity1"></activity>
</application>


<uses-sdk android:targetSdkVersion="4" android:minSdkVersion="3"></uses-sdk>
</manifest> 